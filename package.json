{
  "name": "rest-express",
  "version": "1.0.0",
  "main": "electron/main.cjs",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development VITE_BACKEND_MODE=api tsx server/index.ts",
    "build": "vite build",
    "build:vercel": "cross-env VITE_BACKEND_MODE=local VITE_API_BASE_URL= vite build",
    "preview": "vite preview",
    "test": "node --import dotenv/config --import tsx --test \"tests/*.test.ts\"",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:install": "playwright install chromium",
    "check": "tsc",
    "dev:fullstack": "cross-env NODE_ENV=development VITE_BACKEND_MODE=api tsx server/index.ts",
    "build:fullstack": "tsx script/build.ts",
    "start:fullstack": "cross-env NODE_ENV=production VITE_BACKEND_MODE=api node dist/index.cjs",
    "db:push": "drizzle-kit push",
    "electron:dev": "concurrently -k \"npm:dev:fullstack\" \"npm:electron:run\"",
    "electron:run": "wait-on http://127.0.0.1:5000 && electron electron/main.cjs",
    "electron:build": "npm run build:fullstack && electron-builder --win --x64",
    "electron:pack": "npm run build:fullstack && electron-builder --dir --win --x64"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@jridgewell/trace-mapping": "^0.3.25",
